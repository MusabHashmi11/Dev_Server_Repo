  incPerIndustryMargin =(revisedTurnCap*(indMarGstProgram/100))/12;}
  print("----> incPerIndustryMargin :: " incPerIndustryMargin);
  
  //Eligible EMI for GST Program
  eligEmiGstProgram is a real;
    if(incPerIndustryMargin<>(unknown and unavailable) and foirAsPerGst<>unknown and totalMonthlyOblGSTMethod<>unknown)then{
  eligEmiGstProgram =(incPerIndustryMargin*(foirAsPerGst/100))-totalMonthlyOblGSTMethod;}
  print("----> eligEmiGstProgram :: " eligEmiGstProgram);
  
  //Eligibile Loan Amount in GST Program1
  eligLoAmtGSTProgram1 is a real;
    if(eligEmiGstProgram<>(unknown and unavailable) and emiPerLakh<>unknown)then{
  eligLoAmtGSTProgram1 =(eligEmiGstProgram/emiPerLakh)*100000;}
  print("----> eligLoAmtGSTProgram1 :: " eligLoAmtGSTProgram1);
  
  //EMI basis ABB to EMI ratio
  emiBasisAbbEmiRatio is a real;
  if(monthlyAvgBankBal12m<>unknown and abbToEmiRationForGSTProgram<>unknown and otherBuffer16<>unknown)then{
  //emiBasisAbbEmiRatio =(monthlyAvgBankBal12m*abbToEmiRationForGSTProgram)-otherBuffer16;}
    emiBasisAbbEmiRatio =monthlyAvgBankBal12m/abbToEmiRationForGSTProgram;}
  print("----> emiBasisAbbEmiRatio :: " emiBasisAbbEmiRatio);
  
  //GST Method Eligible Loan Amount 2
  gstMethodEligLoAmt2 is a real;
    if(emiBasisAbbEmiRatio <>unknown and emiPerLakh<>unknown)then{
  gstMethodEligLoAmt2 =(emiBasisAbbEmiRatio/emiPerLakh)*100000;}
  print("----> gstMethodEligLoAmt2 :: " gstMethodEligLoAmt2);
  
  //Final Eligible Loan Amount - GST Program
  finalEligLoAmtGSTProgram is a real;

  print("---->finalEligLoAmtIncMethod*2 ::"finalEligLoAmtIncMethod*2);
    
	if(finalEligLoAmtIncMethod<>unknown and eligLoAmtGSTProgram1<>(unknown and unavailable) and gstMethodEligLoAmt2<>unknown)then{
		finalEligLoAmtGSTProgram =math().min(maxPermissibleLoAmount,math().min(eligLoAmtGSTProgram1,gstMethodEligLoAmt2));
	}
  print("----> finalEligLoAmtGSTProgram :: " finalEligLoAmtGSTProgram);
  
  message.request.application.applicationAdditionalDetails.other.buffer22 =finalEligLoAmtGSTProgram as a string;
 
  //--------------------------------------------------GST Program Loan Eligibility logic ends----------------------------------------------------------------
  
  
  //---->Entering  the Overdraft Logic<-----
  //------------------------------------------------OverDraft Income Method Loan Eligibility logic begins----------------------------------------------------
  
  //Capped PAT for OverDraft logic
  cappedPatOD is a real;
  if(otherBType<>(unknown and unavailable) and otherBuffer6<>(unknown and unavailable))then{
	cappedPatOD =math().min(otherBType,otherBuffer6 + (otherBuffer6*1.5));//Confirmation needed for 150%
  }
  print("---->cappedPatOD ::"cappedPatOD);

  //Capped Depriciation for OverDraft logic
  cappedDepOD is a real;
  if(otherLType<>(unknown and unavailable) and otherBType<>(unknown and unavailable))then{
	cappedDepOD =math().min(otherLType,otherBType+(otherBType*2));
  }
  print("---->cappedDepOD ::"cappedDepOD);
  
  //Cash Profit for OverDraft logic
  cashProfitOD is a real;
  if(cappedPatOD<>(unknown and unavailable) and cappedDepOD<>(unknown and unavailable) and mulIncomeProgram<>(unknown and unavailable) and
  otherPropertyIdentified<>(unknown and unavailable) and otherRiskCategorisation<>(unknown and unavailable) and otherCinNo<>(unknown and unavailable) and
  otherFinalLtvPercentage<>(unknown and unavailable) and otherFinalLoanAmount<>(unknown and unavailable))then{
	cashProfitOD =((cappedPatOD +cappedDepOD)*mulIncomeProgram)+otherPropertyIdentified+otherRiskCategorisation+otherCinNo+otherFinalLtvPercentage+otherFinalLoanAmount;
  }
  print("---->cashProfitOD ::"cashProfitOD);
  
  incEligLoAmt1OD is a real;
  if(cashProfitOD <>unknown and otherBuffer16<>unknown)then {
  incEligLoAmt1OD =cashProfitOD - otherBuffer16;
  }
  print("---->incEligLoAmt1OD ::"incEligLoAmt1OD);
  
  finalEligLoAmtIncMethodOD is a real;
  if(incEligLoAmt1OD <>(unknown and unavailable))then{
	  finalEligLoAmtIncMethodOD =math().min(incEligLoAmt1OD,maxPermissibleLoAmountOD);
  }
  print("---->finalEligLoAmtIncMethodOD ::"finalEligLoAmtIncMethodOD);
    if(finalEligLoAmtIncMethodOD is known)then
    aBlazeApplication.finalEligLoAmtIncMethod_OD =math().round(finalEligLoAmtIncMethodOD, -2) as a string;

  //------------------------------------------------OverDraft Income Method Loan Eligibility logic Ends-----------------------------------------------------------
  
  //------------------------------------------------OverDraft GST Program Loan Eligibility logic begins-----------------------------------------------------------
  
  gstTurnCapOD is a real;
  gstTurnCapOD =math().min(otherSBIMet,additionalBuffer10+(additionalBuffer10*1.5));
  print("---->gstTurnCapOD ::"gstTurnCapOD);
  
  revisedTurnCapOD is a real;
  revisedTurnCapOD =gstTurnCapOD - (otherSBIMet*greatestIndividualContribution/100);
  print("----> revisedTurnCapOD :: " revisedTurnCapOD);
  
  incPerIndustryMarginOD is a real;
  incPerIndustryMarginOD =(revisedTurnCapOD*indMarGstProgram/100)/12;
  print("---->incPerIndustryMarginOD ::"incPerIndustryMarginOD);
  
  eligEmiGstProgramOD is a real;
  eligEmiGstProgramOD =(incPerIndustryMarginOD*foirAsPerGst/100)-totalMonthlyOblGSTMethod;
  print("---->eligEmiGstProgramOD ::"eligEmiGstProgramOD);
  
  eligLoAmtGSTProgram1OD is a real;
  eligLoAmtGSTProgram1OD =(eligEmiGstProgramOD /emiPerLakh)*100000;
  print("---->eligLoAmtGSTProgram1OD ::"eligLoAmtGSTProgram1OD);
  
  finalEligLoAmtGSTProgramOD is a real;
  if(finalEligLoAmtIncMethod<>(unknown and unavailable))then{
	 finalEligLoAmtGSTProgramOD = math().min(eligLoAmtGSTProgram1OD,math().min(maxPermissibleLoAmountOD,(finalEligLoAmtIncMethod*2)));
  }
  print("---->finalEligLoAmtGSTProgramOD ::"finalEligLoAmtGSTProgramOD);
if(finalEligLoAmtGSTProgramOD is known)then
  aBlazeApplication.finalEligLoAmtGSTProgram_OD =math().round(finalEligLoAmtGSTProgramOD,-2) as a string;
  //------------------------------------------------OverDraft GST Program Loan Eligibility logic ends----------------------------------------------------------
  //---->Exiting the Overdraft Logic<-----
  
  
  //---->Entering  the HighTicket Logic<-----
  //----------------------------------------------------HightTicket Income Method  logic begins---------------------------------------------------------------
  
  //Capped PAT for HighTicket logic
  cappedPatHT is a real;
  if(otherBType<>(unknown and unavailable) and otherBuffer6<>(unknown and unavailable))then{
	cappedPatHT =math().min(otherBType,otherBuffer6 + (otherBuffer6*1.5));//Confirmation needed for 150%
  }
  print("---->cappedPatHT ::"cappedPatHT);

  //Capped Depriciation for OverDraft logic
  cappedDepHT is a real;
  if(otherLType<>(unknown and unavailable) and otherBType<>(unknown and unavailable))then{
	cappedDepHT =math().min(otherLType,otherBType+(otherBType*2));
  }
  print("---->cappedDepHT ::"cappedDepHT);
  
  //Cash Profit for OverDraft logic
  cashProfitHT is a real;
  if(cappedPatHT<>(unknown and unavailable) and cappedDepHT<>(unknown and unavailable) and mulIncomeProgramHT<>(unknown and unavailable) and
  otherPropertyIdentified<>(unknown and unavailable) and otherRiskCategorisation<>(unknown and unavailable) and otherCinNo<>(unknown and unavailable) and
  otherFinalLtvPercentage<>(unknown and unavailable) and otherFinalLoanAmount<>(unknown and unavailable))then{
	cashProfitHT =((cappedPatHT +cappedDepHT)*mulIncomeProgramHT)+otherPropertyIdentified+otherRiskCategorisation+otherCinNo+otherFinalLtvPercentage+otherFinalLoanAmount;
  }
  print("---->cashProfitHT ::"cashProfitHT);
  
  incEligLoAmt1HT is a real;
  if(cashProfitHT <>unknown and otherBuffer16<>unknown)then {
  incEligLoAmt1HT =cashProfitHT - otherBuffer16;
  }
  print("---->incEligLoAmt1HT ::"incEligLoAmt1HT);
  
  emiEligLoanAmount1HT is a real;
  if(incEligLoAmt1HT<>unknown and ratePerMonth<>unknown and multiplier<>unknown)then{
  emiEligLoanAmount1HT =( incEligLoAmt1HT * ratePerMonth * multiplier) / divisor;}
  print("----> emiEligLoanAmount1HT :: " emiEligLoanAmount1HT);
  
  //EMI basis ABB to EMI ratio HT
  emiAbbEmiRatioHT is a real;
  if(monthlyAvgBankBal12m<>unknown and abbEmiCapIncProgramHT<>unknown)then{
  emiAbbEmiRatioHT =monthlyAvgBankBal12m/abbEmiCapIncProgramHT;}
  print("----> emiAbbEmiRatioHT :: " emiAbbEmiRatioHT);
  
  //Income Method Eligible Loan Amount 2
  incEligLoAmt2HT is a real;
   if(emiAbbEmiRatioHT<>unknown and emiPerLakh<>unknown)then{
  incEligLoAmt2HT =(emiAbbEmiRatioHT/emiPerLakh)*100000;}
  print("----> incEligLoAmt2HT :: " incEligLoAmt2HT);
  
  //OverDraft IncomeMethod High Ticket
  finalEligLoAmtIncMethodHT is a real;
  if(incEligLoAmt1HT <>(unknown and unavailable) and incEligLoAmt1HT <>(unknown and unavailable))then{
	  finalEligLoAmtIncMethodHT =math().min(math().min(incEligLoAmt1HT,incEligLoAmt2HT),maxPermissibleLoAmountIncHT);
  }
  print("---->finalEligLoAmtIncMethodHT ::"finalEligLoAmtIncMethodHT);
    if(finalEligLoAmtIncMethodHT is known)then
  		aBlazeApplication.finalEligLoAmtIncMethod_HT =math().round(finalEligLoAmtIncMethodHT,-2) as a string;
    
  finalEligLoAmtIncMethodOdHt is a real;
  if(incEligLoAmt1HT <>(unknown and unavailable))then{
	  finalEligLoAmtIncMethodOdHt =math().min(incEligLoAmt1HT,maxPermissibleLoAmountOdIncHT);
  }
  print("---->finalEligLoAmtIncMethodOdHt ::"finalEligLoAmtIncMethodOdHt);
  if(finalEligLoAmtIncMethodOdHt is known)then
    aBlazeApplication.finalEligLoAmtIncMethod_OdHt =math().round(finalEligLoAmtIncMethodOdHt,-2) as a string;
  //-------------------------------------------------------HightTicket Income Method  logic ends--------------------------------------------------------------
  
      //-------------------------------------------------HighTicket Banking Surrogate logic Begins------------------------------------------------------------
  
  //Eligible EMI for Banking Surrogate
  eligEmiBankingSurHT is a real;
    if(monthlyAvgBankBal12m<>unknown and abbToEmiRatioBanksurHT<>(unknown and unavailable) and totalMonthlyOblBankSurMethod <>unknown)then{
  eligEmiBankingSurHT =(monthlyAvgBankBal12m-totalMonthlyOblBankSurMethod)/abbToEmiRatioBanksurHT;
  }
  print("----> eligEmiBankingSurHT :: " eligEmiBankingSurHT);
  
  //Banking Surrogate Eligible Loan Amount 1
  bankingSurEligLoAmt1HT is a real;
    if(eligEmiBankingSur <>unknown and emiPerLakh<>unknown)then{
  bankingSurEligLoAmt1HT =(eligEmiBankingSurHT/emiPerLakh)*100000;}
  print("----> bankingSurEligLoAmt1HT :: " bankingSurEligLoAmt1HT);
  
  //Eligibile Loan Amount Basis Credit to Loan ratio
  eligLoAmtCreditLoRatioHT is a real;
    if(otherGBIMet <>unknown and creditToLoanRatio<>unknown)then{
  eligLoAmtCreditLoRatioHT = otherGBIMet/creditToLoanRatioHT;}
  print("----> eligLoAmtCreditLoRatioHT :: " eligLoAmtCreditLoRatioHT);
  
  //Final Eligible Loan Amount - Banking Method
  finalEligLoAmtBankingSurHT is a real;
  if(finalEligLoAmtIncMethodHT<>unknown and bankingSurEligLoAmt1HT<>unknown and eligLoAmtCreditLoRatioHT<>unknown )then{
  finalEligLoAmtBankingSurHT =math().min(maxPermissibleLoAmountBankSurHT,math().min(bankingSurEligLoAmt1HT,eligLoAmtCreditLoRatioHT));
  //message.request.application.applicationAdditionalDetails.buffer9 =finalEligLoAmtBankingSur as a string;
  print("----> finalEligLoAmtBankingSurHT :: " finalEligLoAmtBankingSurHT);
  }
    if(finalEligLoAmtBankingSurHT is known)then
      aBlazeApplication.finalEligLoAmtBankingSur_HT =math().round(finalEligLoAmtBankingSurHT,-2) as a string;
  
  //---------------------------------------------------HighTicket Banking Surrogate logic ends------------------------------------------------------------
  
  //---->Exiting  the HighTicket Logic<-----

  //---------------------------------------------------------Final Offer Logic Begins--------------------------------------------------------------------------
  
  //Eligible Loan Amount
  eligLoAmount is a real;
  if(eligibilityMethod ="" or eligibilityMethod =unavailable or eligibilityMethod ="")then{
    eligLoAmount =math().max(finalEligLoAmtIncMethod,math().max(finalEligLoAmtBankingSur,finalEligLoAmtGSTProgram));
	print("----> eligLoAmount :: " eligLoAmount);	
  }else if(eligibilityMethod =(ignoring case)"INCOMEMETHOD")then{
      eligLoAmount =finalEligLoAmtIncMethod;  
    }
	else if(eligibilityMethod =(ignoring case)"BANKINGSURROGATE")then{
      eligLoAmount =finalEligLoAmtBankingSur;
    }
	else if(eligibilityMethod =(ignoring case)"GSTPROGRAM")then{
      eligLoAmount =finalEligLoAmtGSTProgram;  
    }
	else if(eligibilityMethod =(ignoring case)"Overdraft Income Method")then{
      eligLoAmount =finalEligLoAmtIncMethodOD;  
    }
	else if(eligibilityMethod =(ignoring case)"Overdraft GST Method")then{
      eligLoAmount =finalEligLoAmtGSTProgramOD;
    }
	else if(eligibilityMethod =(ignoring case)"Income Method - HT Program")then{
	  eligLoAmount =finalEligLoAmtIncMethodHT;
  }
	else if(eligibilityMethod =(ignoring case)"Overdraft Income - HT Program")then{
	  eligLoAmount =finalEligLoAmtIncMethodOdHt;
  }
	else if(eligibilityMethod =(ignoring case)"Banking Method - HT Program")then{
	  eligLoAmount =finalEligLoAmtBankingSurHT;
  }
  
  print("----> eligLoAmount :: " eligLoAmount);
  message.request.application.applicationAdditionalDetails.finalOffer.MAXLOELIGINC =math().round(eligLoAmount, -2);
    
	if(rentalIncomeProgram =unknown and otherBuffer2 is known and eligLoAmount is known)then{
		rentalIncomeProgram =math().min(otherBuffer2,eligLoAmount);
	}
	print("---->rentalIncomeProgram ::"rentalIncomeProgram);
	message.request.application.applicationAdditionalDetails.other.rentalIncomeProgram =rentalIncomeProgram as a string;
	
   finalLoanEMI =(rentalIncomeProgram * ratePerMonth * multiplier) / divisor;
    print("----------------->FinalLoanEmi::"finalLoanEMI);
   message.request.application.applicationAdditionalDetails.finalOffer.EMIFINOFF =math().round(finalLoanEMI,-2);
  print("----> finalLoanEMI :: "message.request.application.applicationAdditionalDetails.finalOffer.EMIFINOFF);
    
      //----------------------------------------------------FinalFoir Logic starts---------------------------------------------------------
	  
  finalFoir is a real initially 0;
    if(eligibilityMethod =(ignoring case)"INCOMEMETHOD")then{
      finalFoir =((otherBuffer16 + rentalIncomeProgram)/cashProfit)*100;
	  print("---->Income finalFoir ::" finalFoir);  
    }
	
    else if(eligibilityMethod =(ignoring case)"BANKINGSURROGATE")then{
      finalFoir =((finalLoanEMI + totalMonthlyOblBankSurMethod)/monthlyAvgBankBal12m)*100;
	  print("---->Banking finalFoir ::"finalFoir);
    }
	
    else if(eligibilityMethod =(ignoring case)"GSTPROGRAM" or eligibilityMethod =(ignoring case)"Overdraft GST Method")then{
      finalFoir =((finalLoanEMI + totalMonthlyOblGSTMethod)/ incPerIndustryMargin)*100;
	  print("----> GST finalFoir ::" finalFoir);  
    }
    
     else if(eligibilityMethod =(ignoring case)"Overdraft Income Method")then{
     	 finalFoir =((otherBuffer16 + rentalIncomeProgram)/cashProfit)*100;
	 	 print("---->Income finalFoir ::" finalFoir);  
      }
	  
	 else if(eligibilityMethod =(ignoring case)"Income Method - HT Program" or eligibilityMethod =(ignoring case)"Overdraft Income - HT Program")then{
		finalFoir =((otherBuffer16 + rentalIncomeProgram)/cashProfitHT)*100;
		print("---->Income finalFoir HT ::" finalFoir);  
	}
	
	else if(eligibilityMethod =(ignoring case)"Banking Method - HT Program")then{
      finalFoir =((finalLoanEMI + totalMonthlyOblBankSurMethod)/monthlyAvgBankBal12m)*100;
	  print("---->Banking finalFoir HT ::" finalFoir);
    }
    
    message.request.application.applicationAdditionalDetails.finalOffer.FINALFOIR =math().round(finalFoir,-2);
	//appending calculated variables
	fn_AppendCalculatedVariable(message, "finalEligLoAmtIncMethodOD", "real", "BL", "BL", "Application", "BL", finalEligLoAmtIncMethodOD as a string);

 //----------------------------------------------------------Final Offer Logic ends---------------------------------------------------------
  }

  print("++++> Exiting function :: fn_BL_EligibilityCalculation");
  print("");
  
}catch an Exception with {
  aBlazeApplication.fn_BL_EligibilityCalculationException = true;  
  print("\n***Exception :: \n>>@@@<<\n" evaluationException().toString()"\n>>@@@<<\n");  
